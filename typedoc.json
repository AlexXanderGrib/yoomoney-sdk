{
  "name": "YooMoney SDK",
  "out": "./docs/api",
  "entryPoints": [
    "./src/index.ts",
    "./src/payment-form-builder.ts",
    "./src/api.ts",
    "./src/auth.ts",
    "./src/notifications.ts",
    "./src/api.types.ts"
  ],
  "exclude": [
    "src/__test__",
    "**/node_modules/**"
  ],
  "externalPattern": [
    "**/node_modules/**"
  ],
  "excludePrivate": false,
  "hideGenerator": false,
  "readme": "README.md",
  "theme": "default",
  "tsconfig": "./tsconfig.json",
  "plugin": [
    "typedoc-plugin-markdown"
  ],
  "outputFileStrategy": "modules",
  "cleanOutputDir": true
}